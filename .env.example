# Environment Configuration
NODE_ENV=production
PORT=3001

# Application Configuration  
USE_API_DATA=true
REPORTS_ENABLED=false

# RADE API Configuration
RADE_API_BASE_URL=https://api.radeapp.com
RADE_API_TOKEN=your_rade_production_token_here

# Z-API WhatsApp Configuration
ZAPI_INSTANCE_ID=your_zapi_instance_id_here
ZAPI_TOKEN=your_zapi_token_here
ZAPI_CLIENT_TOKEN=your_zapi_client_token_here
ZAPI_BASE_URL=https://api.z-api.io

# Gemini AI Configuration
GOOGLE_GENERATIVE_AI_API_KEY=your_gemini_api_key_here

# Database Configuration (if needed)
DATABASE_URL=postgresql://user:password@localhost:5432/chatbot_db

# Logging Configuration
LOG_LEVEL=info
LOG_FILE_PATH=/app/logs/application.log

# Security Configuration
CORS_ORIGIN=https://yourdomain.com
JWT_SECRET=your_jwt_secret_here

# Health Check Configuration
HEALTH_CHECK_INTERVAL=30000